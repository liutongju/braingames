{% extends "braingames/basegame.jinja" %}

{% block page_title %}Find the Icon{% endblock %}

{% block header %}
	<script src="{{ gamestatic('js/js.cookie.js') }}"></script>
  <script src="{{ gamestatic('js/Snap.svg/dist/snap.svg-min.js') }}"></script>
  <link href="{{ gamestatic('css/jspsych.css') }}" rel="stylesheet" />
  <script src="{{ gamestatic('js/jspsych.js') }}"></script>
  <script src="{{ gamestatic('js/plugins/jspsych-instructions.js') }}"></script>
  <script src="{{ gamestatic('js/plugins/jspsych-single-audio.js') }}"></script>
  <script src="{{ gamestatic('js/plugins/jspsych-single-stim.js') }}"></script>
  <script src="{{ gamestatic('js/plugins/jspsych-survey-multi-choice.js') }}"></script>
  <script src="{{ gamestatic('js/plugins/jspsych-survey-text.js') }}"></script>
	<script src="{{ gamestatic('js/plugins/jspsych-text.js') }}"></script>  
  <script src="{{ gamestatic('js/plugins/jspsych-visual-search-circle.js') }}"></script>
{% endblock %}

{% block game_script %}
  {% if request.user_agent.is_mobile %}
    {% include 'icon_vs_1/static/js/nomobile.js' %}
  {% else %}
  	{% include 'icon_vs_1/game.js' %}
  {% endif %}
{% endblock %}

